{
    "name": "clarkwinkelmann/flarum-ext-jwt-cookie",
    "description": "Read test JSON web token from cookie",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.2",
        "firebase/php-jwt": "^6.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "authors": [
        {
            "name": "Clark Winkelmann",
            "homepage": "https://clarkwinkelmann.com/",
            "email": "clark.winkelmann@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/clarkwinkelmann/flarum-ext-jwt-cookie"
    },
    "autoload": {
        "psr-4": {
            "ClarkWinkelmann\\JWTCookie\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "JWT Cookie",
            "category": "feature",
            "icon": {
                "name": "fas fa-cookie",
                "backgroundColor": "#684ba6",
                "color": "#fff"
            }
        }
    }
}
